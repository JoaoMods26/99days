		local col = (i-1) % 2
		btn.Size = UDim2.new(0.48, 0, 0, 45)
		btn.Position = UDim2.new(col * 0.51 + 0.01, 0, 0, 185 + row * 55)
		btn.Text = buttonData.text .. ": OFF"

	task.wait(2)
	createNotification("Jmods 99", 4, Color3.fromRGB(60, 120, 60))
	task.wait(4.5)
	createNotification("ðŸŽ® CONTROLES:\nF1: GUI | F2: Collect | F3: Noclip\nF4: Fly | F5: InfJump | F6: Fullbright\nF7: Auto-Farm | F8: Anti-AFK", 6, Color3.fromRGB(100, 150, 255))
	task.wait(6.5)
	createNotification("ðŸ”§ NUEVAS MEJORAS:\nâ€¢ Funciones no se activan solas\nâ€¢ Mejor sistema de notificaciones\nâ€¢ Estados de botones corregidos\nâ€¢ Manejo de errores mejorado", 5, Color3.fromRGB(255, 200, 100))
end)

